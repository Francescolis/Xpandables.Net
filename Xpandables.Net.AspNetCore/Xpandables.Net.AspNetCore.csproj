<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<LangVersion>preview</LangVersion>

		<Title>Xpandables.Net.AspNetCore</Title>
		<Description>Provide basic objects.</Description>
		<Company>Kamersoft</Company>
		<Authors>Francescolis</Authors>

		<AssemblyName>Xpandables.Net.AspNetCore</AssemblyName>
		<RootNamespace>Xpandables.Net.AspNetCore</RootNamespace>
		<PackageId>Xpandables.Net.AspNetCore</PackageId>
		<PackageProjectUrl>https://github.com/Francescolis/Xpandables.Net</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Francescolis/Xpandables.Net</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>

		<PackageIcon>Xpandables.Net.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageTags>IAddService, LazyResolved</PackageTags>
		<NeutralLanguage>en-US</NeutralLanguage>
		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>

		<Version>1.0.0-preview.1</Version>
		<AssemblyVersion>1.0.0.0</AssemblyVersion>
		<FileVersion>1.0.0.0</FileVersion>
		<PackageVersion>1.0.0-preview.1</PackageVersion>

		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>

		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<IncludeSource>true</IncludeSource>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>

		<ApplicationIcon>Xpandables.Net.ico</ApplicationIcon>
		<Platforms>x64</Platforms>

		<SignAssembly>true</SignAssembly>
		<AssemblyOriginatorKeyFile>Xpandables.Net.snk</AssemblyOriginatorKeyFile>

		<Deterministic>true</Deterministic>
		<ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
		<DebugType>portable</DebugType>

		<EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
		<AnalysisLevel>latest</AnalysisLevel>
		<EnableNETAnalyzers>true</EnableNETAnalyzers>
		<RunAnalyzersDuringLiveAnalysis>true</RunAnalyzersDuringLiveAnalysis>
		<EnableTrimAnalyzer>true</EnableTrimAnalyzer>
		<AnalysisMode>All</AnalysisMode>
		<EnablePackageValidation>false</EnablePackageValidation>
		<EnableUnsafeBinaryFormatterSerialization>false</EnableUnsafeBinaryFormatterSerialization>
		<IsTrimmable>true</IsTrimmable>
		<IsAotCompatible>true</IsAotCompatible>
		<WarningLevel>9999</WarningLevel>
		<Copyright>Copyright © Kamersoft 2025</Copyright>
		<RootNamespace>Xpandables.Net</RootNamespace>
	</PropertyGroup>

	<ItemGroup>
	  <Content Include="Xpandables.Net.ico" />
	</ItemGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>

	<ItemGroup>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\Xpandables.Net.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="System.ComponentModel.Composition" Version="10.0.0-rc.2.25502.107" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Xpandables.Net.Async.AspNetCore\Xpandables.Net.Async.AspNetCore.csproj" />
	  <ProjectReference Include="..\Xpandables.Net.Events.Pipelines\Xpandables.Net.Events.Pipelines.csproj" />
	  <ProjectReference Include="..\Xpandables.Net.ExecutionResults.AspNetCore\Xpandables.Net.ExecutionResults.AspNetCore.csproj" />
	  <ProjectReference Include="..\Xpandables.Net.Repositories.Pipelines\Xpandables.Net.Repositories.Pipelines.csproj" />
	  <ProjectReference Include="..\Xpandables.Net.Tasks\Xpandables.Net.Tasks.csproj" />
	  <ProjectReference Include="..\Xpandables.Net.Validators.AspNetCore\Xpandables.Net.Validators.AspNetCore.csproj" />
	  <ProjectReference Include="..\Xpandables.Net.Validators.Pipelines\Xpandables.Net.Validators.Pipelines.csproj" />
	</ItemGroup>
	
</Project>
