<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <OutputType>Exe</OutputType>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsPackable>false</IsPackable>
	  <LangVersion>latest</LangVersion>
    <Configurations>Debug;Release</Configurations>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <Platforms>x64</Platforms>
    <AssemblyName>Xpandables.Net.Benchmarking</AssemblyName>
    <RootNamespace>Xpandables.Net.Benchmarking</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="BenchmarkDotNet" Version="0.15.6" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\System.Net.Http.AsyncPaged\System.Net.Http.AsyncPaged.csproj" />
    <ProjectReference Include="..\System.Text.Json.AsyncPaged\System.Text.Json.AsyncPaged.csproj" />
  </ItemGroup>
</Project>
